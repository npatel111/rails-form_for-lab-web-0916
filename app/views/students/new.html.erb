Student Form
<%= form_for(@student) do |f| %>
  <!-- <label>First Name</label><br> -->
  <%= f.label :first_name %><br>
  <%= f.text_field :first_name %> <br>

  <!-- <label>Last Name</label><br> -->
  <%= f.label :last_name  %>
  <%= f.number_field :last_name %> <br>

  <%= f.submit %>
  <% end %>
